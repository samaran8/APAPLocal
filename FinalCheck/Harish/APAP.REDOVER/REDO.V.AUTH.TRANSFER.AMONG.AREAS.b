    Y.DEST.AREA =  R.NEW(RE.ORD.TRANSFER.TO)
    Y.FROM.COMAPNY = R.NEW(RE.ORD.TRANSFER.FROM)
    Y.TRANS.QNTY = R.NEW(RE.ORD.TRANSFER.QUANTITY)

