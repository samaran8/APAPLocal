    OFSFUNCTION='I'
    PROCESS='PROCESS'
    OFS.SOURCE.ID='UPDATE.PASSBOOK'
    OFSVERSION=''
    GTSMODE=''
    NO.OF.AUTH='0'
    TRANSACTION.ID= ''
    OFSSTRING=''
    Y.DEST.CODE = ''
    Y.DEST.AREA =  R.NEW(RE.ORD.TRANSFER.TO)
    Y.DEST.CODE = R.NEW(RE.ORD.BRANCH.TRAN.CODE)
