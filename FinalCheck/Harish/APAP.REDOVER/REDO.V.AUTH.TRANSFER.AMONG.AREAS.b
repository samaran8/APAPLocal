            SEL.LIST.1 = SEL.LIST.1
            CALL F.READ(FN.REDO.H.DEPOSIT.RECEIPTS,SEL.LIST.1,R.REDO.H.DEPOSIT.RECEIPTS,F.REDO.H.DEPOSIT.RECEIPTS,Y.ERR.APP)
