SUBROUTINE L.APAP.VAL.CARD.TYPE.RT
    $INSERT I_COMMON
    $INSERT I_EQUATE
    $INSERT I_F.CARD.TYPE

    FN.CT = "F.CARD.TYPE"
    F.CT = ""
    R.CT = ""
    CT.ERR = ""
    CALL OPF(FN.CT,F.CT)

    Y.ID = ''
    Y.ID = COMI

    CALL F.READ(FN.CT,Y.ID,R.CT, F.CT, CT.ERR)
    IF R.CT EQ '' THEN
        MESSAGE = "REGISTRO NO EXISTE EN CARD.TYPE."
        E = MESSAGE
        ETEXT = E
        CALL ERR
    END

END
